!
! emacs resources
!
emacs*Background: DarkSlateGray
emacs*Foreground: Wheat
emacs*pointerColor: Orchid
emacs*cursorColor: Orchid
emacs*bitmapIcon: on
!emacs*font: fixed
emacs.geometry: 80x25
!
! gnuplot resources
!
gnuplot*background: black
gnuplot*textColor: wheat
gnuplot*borderColor: white
gnuplot*axisColor: white
!
! xfm resources
!
xfm*defaultEditor: textedit
xfm*defaultViewer: textedit
!
! xterm resources
!
xterm*background: black
xterm*foreground: wheat
xterm*scrollBar:        True
xterm*saveLines:        1000
xterm*loginShell:       True
xterm*font1: lucidasanstypewriter-8
xterm*font2: lucidasanstypewriter-10
xterm*font3: lucidasanstypewriter-12
xterm*font4: lucidasanstypewriter-14
xterm*font5: lucidasanstypewriter-18
xterm*font6: lucidasanstypewriter-24
